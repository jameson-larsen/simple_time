<script>

    $(function() {
        var calendarEl = document.getElementById('calendar');
        var userPunches = $('.user_punches').data('punches')
        userPunches = userPunches.filter(i => i.duration >= 0.25).map(i => {
            return {id: i.id, start: i.in, end: i.out, name: "punch", color: '#a7c6c9'}
        })
        var calendar = new FullCalendar.Calendar(calendarEl, {
            initialView: 'timeGridWeek',
            events: userPunches,
            allDaySlot:false,
            scrollTime:'08:00:00'
        });
        calendar.render();
    });
</script>
<div id='calendar'></div>
